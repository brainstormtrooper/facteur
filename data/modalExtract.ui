<interface>
  <!-- interface-name gtk4uis.ui -->
  <requires lib="gtk" version="4.6" />

  <template class="modalExtract" parent="GtkBox">
    <property name="orientation">vertical</property>
    <property name="vexpand">true</property>
    <property name="hexpand">true</property>
    <child>
      <object class="GtkBox" id="extractCheckAllRow">

        <property name="orientation">horizontal</property>
        <property name="vexpand">false</property>
        <property name="hexpand">true</property>
        <child>
          <object class="GtkCheckButton" id="extractCheckAllBtn">
            <property name="label">Select All</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkScrolledWindow" id="extractScroll">
        <property name="hscrollbar-policy">never</property>
        <property name="vexpand">true</property>
        <property name="hexpand">true</property>
        
      </object>
    </child>
  </template>
</interface>