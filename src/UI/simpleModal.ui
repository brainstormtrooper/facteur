<interface>
  <!-- interface-name gtk4uis.ui -->
  <requires lib="gtk" version="4.6"/>
  <template class="SimpleModal" parent="GtkWindow">
    <property name="destroy-with-parent">True</property>
    <property name="modal">True</property>
    <child>
      <object class="GtkBox" id="dialog_area">
        <property name="css-classes">dialog_content_box</property>
        <property name="orientation">vertical</property>
        <property name="vexpand">true</property>
        <property name="hexpand">true</property>
        <child>
          <object class="GtkLabel" id="dialog_label">
            <property name="justify">center</property>
            <property name="label">Test</property>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="dialog_content_area">
            <property name="orientation">vertical</property>
            <property name="vexpand">true</property>
            <property name="hexpand">true</property>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="dialog_action_area">
            <property name="css-classes">dialog_action_area</property>
            <property name="orientation">horizontal</property>
            
            <property name="hexpand">true</property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>